{
  "expo": {
    "name": "dts-mobile",
    "slug": "dts-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["ACCESS_BACKGROUND_LOCATION","ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION","CAMERA","FOREGROUND_SERVICE","USE_FINGERPRINT","USE_BIOMETRIC","READ_EXTERNAL_STORAGE","WRITE_EXTERNAL_STORAGE"],        
      "softwareKeyboardLayoutMode" : "pan"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
